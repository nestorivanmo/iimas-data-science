   "outputs": [],
   "source": [
    "target = function(x) {\n",
    "    dbeta(x, 0.35, 0.65)\n",
    "}\n",
    "metropolis = function(x, alpha=1) {\n",
    "    y = x + rnorm(1, mean=0, sd=1)\n",
    "    if (runif(1) > target(y) / target(x)) y = x\n",
    "    return (y)\n",
    "}"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "T = 10^4\n",
    "x = rep(0.2, T)\n",
    "for (t in 2:T) x[t] = metropolis(x[t-1])"
   ]
  },
