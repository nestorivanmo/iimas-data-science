find_x <- function(data, b, sup) {
x <- 0
S <- c()
for (d in data) {
if (sup) {
if (d <= b) {
S <- c(S, d)
}
} else {
if (d >= b) {
S <- c(S, d)
}
}
}
if (sup) {
return(max(S))
} else {
return(min(S))
}
}
